class Solution {
    public int findNumbers(int[] nums) {
       int n = nums.length;
       int count = 0;

       for(int i=0 ; i<n ; i++){
         int digits=0;
        
         if(nums[i]==0){
            return digits = 1;
         }else{
            while(nums[i]>0){
                nums[i]=nums[i]/10;
                digits++;
            }
         }
         if(digits%2==0){
            count++;
         }
         
       }

       return count;

    }
}
