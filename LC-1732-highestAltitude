// brute force 
class Solution {
    public int largestAltitude(int[] gain) {
        int n = gain.length;
        int start=0;
        int max=0;
        

        for(int i=0 ; i<n ; i++){
            gain[i]=start+gain[i];
            start=gain[i];
        }

        for(int i=0;i<n;i++){
            max=Math.max(max,gain[i]);
        }
        

        return max;

        
    }

    
}
