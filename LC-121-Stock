class Solution {
    public int maxProfit(int[] prices) {
        int n=prices.length;
        int min=prices[0];
        int profit=0;
        
        
       for(int i=0; i<n; i++ ){
        if(prices[i] < min){
            min = prices[i];
           
        }

        int currentprofit=prices[i]-min;
        if(currentprofit>profit){
            profit = currentprofit;
        }

       }
        return profit;
    }
}
